    "history": "catalog:",
    "i18next": "catalog:",
    "js-base64": "catalog:",
    "localstorage-memory": "catalog:",
    "lodash": "catalog:",
    "marked": "catalog:",
    "marked-highlight": "catalog:",
    "normalize.css": "catalog:",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.49.3",
    "react-i18next": "catalog:",
    "react-json-view": "catalog:",
    "react-markdown": "catalog:",
    "react-redux": "catalog:",
    "react-router": "catalog:",
    "react-router-dom": "catalog:",
    "semver": "catalog:",
    "react-virtualized": "catalog:",
    "redux": "catalog:",
